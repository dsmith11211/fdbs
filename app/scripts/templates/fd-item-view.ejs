<div>
	<form role="form">
		<div class="form-group">
		    <label for="item-title">Title</label>
		    <input type="text" class="form-control" placeholder="<%= title %>" id="item-title" val="<%= title %>"/>
		</div>
		<div class="form-group">
		    <label for="item-description">Description</label>
		    <textarea class="form-control" rows="5" placeholder="<%= description %>" id="item-description" val="<%= description %>"/>
		</div>
		<div class="form-group">
		    <label for="item-description">Internal Notes</label>
		    <textarea class="form-control" rows="3" placeholder="<%= dealerInternalNotes %>" id="item-description" val="<%= dealerInternalNotes %>"/>
		</div>
	</form>
</div>

